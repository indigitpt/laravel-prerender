{
  "name": "indigit/laravel-prerender",
  "description": "Laravel middleware for prerendering javascript-rendered pages on the fly for SEO.",
  "keywords": ["laravel", "prerender"],
  "license": "MIT",
  "authors": [
    {
      "name": "Pedro Martins",
      "email": "support@indigit.pt"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-json": "*",
    "illuminate/support": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "symfony/psr-http-message-bridge": "^1.0|^2.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Indigit\\LaravelPrerender\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Indigit\\LaravelPrerender\\LaravelPrerenderServiceProvider"
      ]
    }
  }
}
